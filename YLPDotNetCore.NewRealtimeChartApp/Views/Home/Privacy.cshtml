@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<p>Use this page to detail your site's privacy policy.</p>

<div class="m-5 p-5 border border-1">
    <input type="text" name="name" class="form-control" id="txtInput" />
</div>

@section scripts{
        <script src="~/js/signalr/dist/browser/signalr.js"></script>
    <script>
        var connection = new signalR.HubConnectionBuilder().withUrl("/chartHub").build();

        connection.on("ClientReceiveEvent", function (data) {
            console.log(data);
            $('#txtInput').val(data);
        });

        connection.start().then(function () {
        }).catch(function (err) {
            return console.error(err.toString());
        });

    </script>
}
